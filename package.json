{
  "name": "aslan",
  "version": "1.0.0",
  "description": "Payment infrastructure for AI agents - Like the great lion, Aslan guides AI agents to accomplish their missions in the real world",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup-stripe": "curl -X POST http://localhost:3000/api/setup-products",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate deploy",
    "db:migrate:dev": "npx prisma migrate dev",
    "db:studio": "npx prisma studio",
    "build": "npx prisma generate",
    "postinstall": "npx prisma generate",
    "railway:deploy": "node scripts/railway-deploy.js",
    "security:validate": "node scripts/validate-security.js",
    "security:generate-secrets": "node scripts/validate-security.js --generate-secrets",
    "security:report": "curl -s http://localhost:3000/api/security | jq .",
    "test:security": "npm run security:validate && echo 'Security validation passed!'",
    "railway:setup": "node scripts/railway-setup.js"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "bcryptjs": "^3.0.2",
    "connect-redis": "^7.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "firebase": "^11.8.1",
    "helmet": "^7.2.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "rate-limiter-flexible": "^7.1.1",
    "redis": "^4.7.1",
    "sendgrid": "^5.2.3",
    "stripe": "^14.12.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "prisma": "^5.7.0"
  },
  "keywords": [
    "ai",
    "payments",
    "stripe",
    "agents",
    "aslan",
    "automation",
    "fintech"
  ],
  "author": "Aslan Technologies",
  "license": "MIT",
  "homepage": "https://aslanpay.xyz",
  "repository": {
    "type": "git",
    "url": "https://github.com/coltonsakamoto/aslanpay.git"
  },
  "bugs": {
    "url": "https://github.com/coltonsakamoto/aslanpay/issues",
    "email": "support@aslanpay.xyz"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
